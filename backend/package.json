    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "prisma": "^6.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
